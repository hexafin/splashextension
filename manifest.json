{
  "manifest_version": 2,
  "name": "RCR",
  "description": "React, Chrome, and Redux example",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "RCR Example"
  },
  "permissions": [ "activeTab", "tabs", "webRequest", "webNavigation", "storage", "cookies", "http://*/*", "https://*/*" ]
}


